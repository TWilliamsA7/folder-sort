{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 24,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "ucrt64-debug",
      "displayName": "MSYS2 UCRT64 Debug",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "BUILD_TESTS": "ON"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "ucrt64-debug",
      "configurePreset": "ucrt64-debug"
    }
  ],
  "testPresets": [
    {
      "name": "ucrt64-debug",
      "configurePreset": "ucrt64-debug",
      "output": {
        "outputOnFailure": true
      }
    }
  ]
}
